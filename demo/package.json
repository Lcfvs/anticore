{
  "name": "anticore-demo",
  "private": true,
  "version": "1.0.3",
  "description": "",
  "type": "module",
  "scripts": {
    "start": "npm run bundle:dev && node src/server.js --po",
    "bundle:dev": "npm run bundle:css && npm run bundle:js && npm run bundle:svg",
    "bundle:prod": "npm run bundle:dev -- --minify",
    "bundle:css": "esbuild --bundle --outdir=./dist src/assets/css/style.css",
    "bundle:js": "esbuild --bundle --format=esm --outdir=./dist src/assets/js/main.js",
    "bundle:svg": "esbuild --loader:.svg=file --asset-names=[name] --outdir=./dist src/assets/img/favicon.svg"
  },
  "dependencies": {
    "@lcf.vs/dom-engine": "^5.2.4",
    "anticore": "^4.5.3",
    "anticore-tree-view": "^1.1.0",
    "dotenv": "^10.0.0",
    "fastify": "^3.20.2",
    "fastify-multipart": "^4.0.7",
    "fastify-static": "^4.2.3"
  },
  "engines": {
    "node": ">=14.x"
  },
  "devDependencies": {
    "esbuild": "^0.12.21",
    "eslint": "^7.28.0",
    "open": "^8.2.0"
  },
  "author": "Lcf.vs",
  "license": "MIT"
}
